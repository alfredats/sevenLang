import java.util.Scanner;

public class Calculator {
  public static void main(String[] args) {
    Scanner input = new Scanner(System.in);
    int x; int y; double f; double g;


    System.out.println("List of operations: add subtract multiply divide alphabetize");
    System.out.println("Enter an operation:");
    String op = input.next().toLowerCase();

    switch (op) {
      case ("alphabetize"):
        System.out.println("Enter two words:");
        String s1 = input.next();
        String s2 = input.next();
        int cmpstr = s1.compareToIgnoreCase(s2);
        if (cmpstr == 0) {
          System.out.println("Chicken or Egg");
        }
        else if (cmpstr > 0) {
          System.out.printf("%s comes before %s lexicographically\n", s2, s1);
        }
        else {
          System.out.printf("%s comes before %s lexicographically\n", s1, s2);
        }
        break;
      case ("add"):
        System.out.println("Enter two integers:");
        int x = input.nextInt();
        int y = input.nextInt();
        System.out.printf("Answer: %d\n", x + y);
        break;
      case ("subtract"):
        System.out.println("Enter two integers:");
        int x = input.nextInt();
        int y = input.nextInt();
        System.out.printf("Answer: %d\n", x - y);
        break;
      case ("multiply"):
        System.out.println("Enter two doubles:");
        double f = input.nextDouble();
        double g = input.nextDouble();
        System.our.printf("Answer: %.2f\n", f * g);
        break;
      case ("divide"):
        System.out.println("Enter two doubles:");
        double f = input.nextDouble();
        double g = input.nextDouble();
        if ((f != 0) && (g != 0)) {
          System.our.printf("Answer: %.2f\n", f * g);
          break;
        }
      default:
        System.out.println("Invalid input entered. Termintating...");
    }
  }
}
